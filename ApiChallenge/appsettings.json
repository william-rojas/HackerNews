{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "NewsApi": {
    "pageSize" :  "20",
    "baseUri": "https://hacker-news.firebaseio.com/v0/",
    "endpoints": [
      {
        "name": "item",
        "uri": "item/{}.json?print=pretty"
      },
      {
        "name": "maxitem",
        "uri": "maxitem.json?print=pretty"
      }
    ]
  },

  "AllowedHosts": "*"
}
